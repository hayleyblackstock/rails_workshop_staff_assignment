

<%= form_for @person do |f| %>

<% if @new_edit_error %>
<div class= "alert alert-danger"><%= @new_edit_error %></div>
<% end %>


<%= f.label :title %>
<%= f.text_field :title %>
<%= f.label :first_name %>
<%= f.text_field :first_name %>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.submit %>
<% end %>
